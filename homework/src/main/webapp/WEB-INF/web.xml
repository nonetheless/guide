  	<servlet-class>main.RegisterServlet</servlet-class>  </servlet>

  
  <servlet-mapping>
  	<servlet-name>AppServlet</servlet-name>
  	<url-pattern>/AppServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RiskAddServlet</servlet-name>
  	<url-pattern>/RiskAddServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping> 
     <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
